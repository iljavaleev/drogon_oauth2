<!DOCTYPE html>
<html>
<%inc#include "models/Client.h" %>
<%c++
    auto para=@@.get<std::unordered_map<std::string,std::string,utils::internal::SafeStringHash>>("parameters");
    auto elses=@@.get<std::vector<std::string>>("else");
    auto client=@@.get<Json::Value>("client");
%>
<head>
    <meta charset="UTF-8">
    <title>[[ title ]]</title>
</head>
<body>
    <%c++ if(para.size()>0){%>
    <H1>Parameters</H1>
    <table border="1">
      <tr>
        <th>name</th>
        <th>value</th>
      </tr>
      <%c++ for(auto iter:para){%>
      <tr>
        <td>{%iter.first%}</td>
        <td><%c++ $$<<iter.second;%></td>
      </tr>
      <%c++}%>
      <tr>
        <td>{% client["client_name"] %}</td>
      </tr>
      <%c++ for(auto iter:elses){%>
      <tr>
        <td>{% iter %}</td>
      </tr>
      <%c++}%>
    </table>
    <%c++ }else{%>
    <H1>no parameter</H1>
    <%c++}%>
</body>
</html>